package  {	import flash.events.Event;	import flash.display.MovieClip;		public class EnterExterior extends MovieClip {		public function EnterExterior() {			this.addEventListener(Event.ADDED_TO_STAGE,onAddedToStage);		}		public function onAddedToStage(event:Event)		{			this.removeEventListener(Event.ADDED_TO_STAGE,onAddedToStage);			this.addEventListener(Event.ENTER_FRAME,onTick);						Game.gameContainer.levelMap.fore_interior.visible = false;			Game.gameContainer.levelMap.back_interior.visible = false;			Game.gameContainer.levelMap.fore_exterior.visible = true;			Game.gameContainer.levelMap.back_exterior.visible = true;		}				public function onTick(event:Event)		{			Game.gameContainer.levelMap.fore_interior.visible = true;			Game.gameContainer.levelMap.back_interior.visible = true;			Game.gameContainer.levelMap.fore_exterior.visible = false;			Game.gameContainer.levelMap.back_exterior.visible = false;		}	}	}