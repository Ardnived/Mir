package  {		import flash.display.MovieClip;			public class Exit extends MovieClip {						public function Exit() {			// constructor code			this.addEventListener(Event.ENTER_FRAME, onTick);		}				function onTick(event:Event){			if(this.hitTestObject(Game.gameContainer.hero)){				Game.removeChild(gameContainer);								var cutScene:CutScene = new CutScene();				cutScene.x = 0;				cutScene.y = 0;				Game.addChild(cutScene);			}		}	}	}