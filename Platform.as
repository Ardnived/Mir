package  {	import flash.display.MovieClip;		public class Platform extends Layout{		var tempPlayer:MovieClip;				public function Platform() {						//intializer			this.addEventListener(event.Event.ADDED_TO_STAGE, onAddedToStage);		}				public function onAddedToStage(event.Event)		{			this.removeEventListener(event.Event.ADDED_TO_STAGE, onAddedToStage);			//tempPlayer = Player.movieclip;			this.addEventListener(event.Event.ENTER_FRAME, checkCollision);		}				public function checkCollision(event.Event)		{			this.hitTestObject(tempPlayer);		}			}	}